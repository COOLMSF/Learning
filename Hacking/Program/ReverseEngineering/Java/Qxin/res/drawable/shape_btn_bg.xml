<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff53a0fd" android:endColor="#ff53a0fd" android:centerColor="#ff53a0fd" />
            <corners android:radius="60.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ff57aff7" android:endColor="#ff57aff7" android:centerColor="#ff57aff7" />
            <corners android:radius="60.0dip" />
        </shape>
    </item>
</selector>